# Load our dotfiles.
for file in ~/.{aliases,aliases_private,exports,functions,functions_private}; do
    [ -r "$file" ] && [ -f "$file" ] && source "$file";
done;
unset file;

# Zsh options.
# See `man zshoptions` for more info.
# Prompting.
setopt PROMPT_CR
setopt PROMPT_SP

# History.
setopt INC_APPEND_HISTORY
setopt SHARE_HISTORY

SAVEHIST=2000
